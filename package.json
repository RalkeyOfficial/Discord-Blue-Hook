{
  "name": "discord-blue-hooks",
  "productName": "discord-blue-hooks",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "build": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },

  "build": {
    "appId": "discord-blue-hooks",
    "win": {
        "target": [
            "nsis"
        ]
    },
    "nsis": {
        "oneClick": true,
        "installerIcon": "./src/assets/icons/blue-hooks.ico",
        "uninstallerIcon": "./src/assets/icons/blue-hooks.ico",
        "uninstallDisplayName": "Blue-Hooks-uninstaller",
        "license": "LICENSE.md",
        "allowToChangeInstallationDirectory": false
    }
  },

  "keywords": [],
  "author": {
    "name": "RalkeyOfficial",
    "email": "Ralkey@outlook.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "discord_blue_hooks"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "16.0.1",
    "electron-builder": "^22.14.5"
  }
}
